
#include<iostream>
using namespace std;
#define N 20
template<class Ta, class Tb, class Tc>
    
int count(Ta& arr, Tb& n, Tc& value)
{
    int count = 0;
    for(int i = 0;i < n; i++)
    {
        if(arr[i] == value)
        {
            count++;
        }
    }
    return count;
}
